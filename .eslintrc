{
    "env": {
        "jasmine": true,
        "node": true,
        "mocha": true,
        "browser": true,
        "builtin": true,
        "es6": true
    },
    "globals": {
    },
    "extends": "airbnb/legacy",
    "rules": {
        "no-unused-vars": 0,
        "no-param-reassign": 0,
        "indent": [
            2,
            4
        ],
        "comma-dangle": [
            2,
            "never"
        ],
        "no-underscore-dange": 0,
        "new-cap": 0,
        "id-length": 0,
        "vars-on-top": 0,
        "func-names": 0,
        "space-before-function-paren": 0,
        "padded-blocks": 0,
        "max-len": [
            2,
            100,
            4,
            {
                "ignoreComments": true,
                "ignoreUrls": true,
                "ignorePattern": "path|require"
            }
        ]
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ]
}
